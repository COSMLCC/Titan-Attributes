command /titanattribute [<text>] [<player>] [<text>]:
	aliases: /titan, /ta, /attribute, /runes
	permission: t.admin
	permission message: "&c&l(!) No Permission!"
	trigger:
		if arg 1 is not set:
			send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
			send "&7"
			send "&7Correct arugument:"
			send "&7/titanattribute give COSMLC D1, D2, D3, D4"
			send "&7"
			send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
			stop
		if arg 1 is not "give":
			send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
			send "&7"
			send "&7Correct arugument:"
			send "&7/titanattribute give COSMLC D1, D2, D3, D4"
			send "&7"
			send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
			stop
		if arg 1 is "give":
			if arg 2 is not set:
				send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
				send "&7"
				send "&7Correct arugument:"
				send "&7/titanattribute give COSMLC D1, D2, D3, D4"
				send "&7"
				send "&a-=-=-=-=-=-=-=- &e&lTitan Attribute &a-=-=-=-=-=-=-=-"
				stop
			if arg 2 is set:
				if arg 3 is "D1":
					give arg-2 a spider eye named "&c&lWeapon Attribute (&8&lDemonic Trap&c&l) &7[I]" with lore "&c&lLevel: &81||&c&lApply: &8100%%||||&c&l""Demonic Trap""||&cDemonic Trap freezes your opponent||&cin place and could NOT be able to||&cMOVE until the ability wears off.||||&7Weapon Attributes can &nONLY&r &7be applied||&7to &cHeroic Weapons &7and only||&7&n1 attribute&r &7can be imbued per weapon.||"
					stop
